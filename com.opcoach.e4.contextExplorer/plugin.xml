<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="com.opcoach.e4.contextExplorer"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="clearPersistedState"
               value="true">
         </property>
         
         
         <property
               name="appName"
               value="Content Explorer">
         </property>
         <property
               name="applicationXMI"
               value="com.opcoach.e4.contextExplorer/Application.e4xmi">
         </property>
   
         
         <property
               name="applicationCSS"
               value="platform:/plugin/com.opcoach.e4.contextExplorer/css/default.css">
         </property>
         <property
               name="lifeCycleURI"
               value="bundleclass://com.opcoach.e4.contextExplorer/com.opcoach.e4.contextExplorer.lifeCycle.PostContextInitializer">
         </property> 
        
      </product>
   </extension>

</plugin>
